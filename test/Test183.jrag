import java.util.*;

aspect Test183 {

  syn lazy final List Root.getDList() = new List();
  syn lazy final List A.getBList() = new List();

  syn lazy B C.decl() = lookupB();
  inh B C.lookupB();

  eq A.getC().lookupB() {
    addB(createB());
    return getB(getNumB()-1);
  }

  public B A.createB() {
    D d = lookupD();
    B b = new B();
    b.setD(d);
    return b;
  }

  inh D A.lookupD();

  eq Root.getA().lookupD() {
    D d = new D();
    addD(d);
    d.name();
    return getD(getNumD()-1);
  }

  syn lazy String D.name() = rootName();
  inh String D.rootName();
  eq Root.getChild().rootName() = getName();

}
